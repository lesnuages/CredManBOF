[
    {
        "name": "credman",
        "help": "Dump credentials using the CredsBackupCredentials API",
        "dependsOn": "coff-loader",
        "entryPoint": "go",
        "extFiles": [
            {
                "os": "windows",
                "files": {
                    "x86": "",
                    "x64": "credman.x64.o"
                }
            }
        ],
        "arguments": [
            {
                "name": "dumpfile-path",
                "desc": "Path to the encrypted dump file",
                "type": "wstring",
                "optional": false
            },
            {
                "name": "pid",
                "desc": "PID of a process started by the target user",
                "type": "int",
                "optional": false
            }
        ]
    }
]